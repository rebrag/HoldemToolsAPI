{
  "AzureStorage": {
    // Storage container your blobs live in
    "ContainerName": "onlinerangedata",
    "ConnectionString": ""
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:yourserver.database.windows.net,1433;Initial Catalog=YourDbName;Persist Security Info=False;User ID=youradminuser;Password=YourPasswordHere;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  // Leave as "*" for dev; tighten in prod if you have a fixed host name
  "AllowedHosts": "*"


}
